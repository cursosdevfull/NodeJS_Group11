    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-uglify-es": "^3.0.0",
    "helmet": "^6.0.1",
    "ioredis": "^5.2.5",
    "jwt-simple": "^0.5.6",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "neverthrow": "^6.0.0",
    "rimraf": "^4.1.1",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "yenv": "^3.0.1"
